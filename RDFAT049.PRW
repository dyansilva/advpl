#INCLUDE "rwmake.ch"

USER FUNCTION RDFAT049()

Local lRet:= .T. 

IF(M->ZF_PRECO > M->ZF_PRCMAX)
	lRet:= .F.
	MSGALERT("PREÇO DE VENDA DEVE SER MENOR OU IGUAL AO PREÇO MÁXIMO!")
	
ENDIF

IF(M->ZF_PRECO < M->ZF_PRCMIN)
	lRet:= .F.
	MSGALERT("PREÇO DE VENDA DEVE SER MAIOR OU IGUAL AO PREÇO MÍNIMO!")
	
ENDIF	

IF(M->ZF_PRCMIN > M->ZF_PRCMAX)
	lRet:= .F.
	MSGALERT("PREÇO MÍNIMO NÃO PODE SER MAIOR QUE PREÇO MÁXIMO")

ENDIF

RETURN lRet